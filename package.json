{
  "scripts": {
    "build": "rollup --config",
    "test": "jest",
    "watch": "jest --watch",
    "lint": "eslint --ext .js {src,test}",
    "prettier": "prettier \"{src,test}/**/*.js\" --write"
  },
  "dependencies": {
    "mime": "2.4.4",
    "tempfile": "3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "0.21.3",
    "buble": "0.20.0",
    "buble-jest": "1.0.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "jest": "25.2.6",
    "lint-staged": "10.1.1",
    "prettier": "2.0.2",
    "rollup": "2.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleNameMapper": {
      "pinsjs(.*)$": "<rootDir>/src/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.js$": "buble-jest"
    },
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "buble": {
    "objectAssign": "Object.assign"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "prettier --write"
    ]
  }
}
